{
    "version": "0.1.0",
    "command": "node",
    "args": ["kha/make", "html5"],
    "problemMatcher": {
        "owner": "haxe",
        "fileLocation": ["relative", "${workspaceRoot}/build"],
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
    }
}
